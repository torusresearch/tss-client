{
  "name": "tss-client-sdk",
  "version": "1.1.0",
  "description": "",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna clean --yes && lerna exec -- rimraf package-lock.json yarn.lock dist/ .rollup.cache/ tsconfig.build.tsbuildinfo .rts2_cache_{cjs,esm}/ *.tgz .eslintcache",
    "dev": "lerna run dev --parallel",
    "depcheck": "lerna run depcheck --parallel",
    "lint:fix": "eslint --fix 'src/**/*.ts' 'src/**/*.js'",
    "pack:lerna": "lerna exec -- 'npm pack'",
    "publish:lerna": "yarn clean && yarn && yarn run build && lerna publish",
    "just:publish:lerna": "lerna publish",
    "serve:example": "cd packages/web-example && npm run serve",
    "serve:example:local": "cd packages/web-example && npm run serve:local",
    "test:ci": "node scripts/e2e.js",
    "test:local": "node scripts/e2e.js --env=local"
  },
  "keywords": [],
  "author": "Web3Auth",
  "license": "ISC",
  "devDependencies": {
    "@babel/runtime": "^7.23.1",
    "@toruslabs/config": "^2.0.2",
    "@toruslabs/eslint-config-typescript": "^3.0.1",
    "@toruslabs/torus-scripts": "^5.0.5",
    "babel-loader": "^9.1.3",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.6",
    "eslint": "^8.50.0",
    "find-process": "^1.4.7",
    "lerna": "^7.3.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "selenium-webdriver": "^4.16.0",
    "typescript": "^5.2.2",
    "vm-browserify": "^1.1.2",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@types/chrome": "^0.0.246",
    "@types/webextension-polyfill": "^0.10.4"
  }
}
